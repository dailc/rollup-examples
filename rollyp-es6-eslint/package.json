{
  "name": "rollyp-es6-eslint",
  "version": "1.0.0",
  "description": "rollyp-es6-eslint",
  "homepage": "https://github.com/dailc/rollup-examples",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node build/build.js",
    "watch": "node build/watch.js",
    "dev": "npm run watch",
    "test:eslint": "npm run eslint:report",
    "eslint": "node_modules/.bin/eslint ./src",
    "eslint:fix": "node_modules/.bin/eslint ./src --fix",
    "eslint:report": "node_modules/.bin/eslint ./src -f html -o ./_report/eslint.html"
  },
  "author": "dailc",
  "license": "GPL-3.0",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-eslint": "^4.0.0",
    "uglify-js": "^3.1.0"
  }
}
